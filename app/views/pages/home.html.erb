
<%= render 'shared/navbar' %>




  <div id="carouselExampleFade" class="carousel slide carousel-fade" data-ride="carousel">
  <div class="carousel-inner">
    <%= simple_form_for :search, url: root_path, method: "GET", html: { class: 'form-inline' } do |f| %>
  <%= f.input :address, input_html: {value: ""} %>
  <%= f.input :begin_date, as: :date %>
  <%= f.input :end_date, as: :date %>
  <%= f.submit "Search", class: "btn btn-primary" %>
  <% end %>
    <div class="carousel-item active">
      <img class="d-block w-100" src="https://images.pexels.com/photos/1149601/pexels-photo-1149601.jpeg?cs=srgb&dl=bicycle-blue-bricks-1149601.jpg&fm=jpg"
        alt="First slide">
    </div>
    <div class="carousel-item">
      <img class="d-block w-100" src="https://images.pexels.com/photos/462036/pexels-photo-462036.jpeg?cs=srgb&dl=adventure-beach-bicycle-462036.jpg&fm=jpg"
        alt="Second slide">
    </div>
    <div class="carousel-item">
      <img class="d-block w-100" src="Christopher Foley [5:52 PM]
https://images.pexels.com/photos/1580234/pexels-photo-1580234.jpeg?cs=srgb&dl=action-balance-bicycle-1580234.jpg&fm=jpg"
        alt="Third slide">
    </div>
  </div>
  <a class="carousel-control-prev" href="#carouselExampleFade" role="button" data-slide="prev">
    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
    <span class="sr-only">Previous</span>
  </a>
  <a class="carousel-control-next" href="#carouselExampleFade" role="button" data-slide="next">
    <span class="carousel-control-next-icon" aria-hidden="true"></span>
    <span class="sr-only">Next</span>
  </a>


